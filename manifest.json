{
  "manifest_version": 2,
  "name": "Ecosia Sync",
  "version": "0.1",
  "description": "An extension that syncs that number of searches done using ecosia",
  "permissions": [
    "storage",
    "cookies",
    "https://www.ecosia.org/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "ecosia@nilsand.re"
    }
  }
}
